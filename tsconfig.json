{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es5",
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "module": "CommonJS",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true
    },
    "exclude": ["node_modules"]
}
  